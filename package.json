{
  "name": "electron-angular-app",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint . --ext .js,.ts,",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "setup:paths": "node ./scripts/setup-paths.js",
    "postinstall": "npm run setup:paths",
    "electron": "ng build && electron .",
    "electron:dev": "concurrently \"npm run start\" \"wait-on http://localhost:4200 && electronmon .\"",
    "electron:publish": "npm run build && electron-builder --publish always",
    "electron:build": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.example.angular-electron",
    "publish": [
      {
        "provider": "github",
        "owner": "ShivendraPorwal",
        "repo": "electron-web-app"
      }
    ],
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/eslint-plugin": "^17.3.0",
    "@angular/animations": "^19.0.5",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@eslint/eslintrc": "^3.1.0",
    "@ng-bootstrap/ng-bootstrap": "~15.0.0",
    "@popperjs/core": "^2.11.8",
    "@primeng/themes": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "@typescript-eslint/parser": "^7.6.0",
    "bootstrap": "^5.3.3",
    "dwt": "^18.5.1",
    "electron-updater": "^6.3.9",
    "electronmon": "^2.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.0",
    "eslint-plugin-import": "^2.29.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.12",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@types/dwt": "^16.1.7",
    "@types/electron": "^1.4.38",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.1.0",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "wait-on": "^8.0.1"
  }
}
